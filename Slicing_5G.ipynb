    "#     return (rho**nb_of_servers) / (math.factorial(nb_of_servers) * sum_res)\n",
    "\n",
    "\n",
    "def erlang_b_recursive(p, s):\n",
    "\n",
    "    if s == 0:\n",
    "        return 1 \n",
    "    \n",
    "    return (p *  erlang_b_recursive(p, s-1) / (s + (p  * erlang_b_recursive(p, s-1))))\n",
    "    \n",
    "\n",
    "def mean_number_waiting_customers(arrival_rate, service_rate, nb_of_servers):\n",
    "\n",
    "    sum_res = 0\n",
    "    p = arrival_rate / service_rate\n",
    "\n",
    "    for n in range(nb_of_servers + 1):\n",
    "        P = erlang_b_recursive(p, n) \n",
    "        print(f\"n = {n}, P[n] = {P}\")\n",
    "        sum_res += n * P\n",
    "\n",
    "    return sum_res"
