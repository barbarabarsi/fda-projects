    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def plot_mean_clients_evolution():\n",
    "    S = 4  # Número de servidores\n",
    "    lamb = 100  # Variação da taxa de chegada (lambda)\n",
    "    mu = 10  # Variação da taxa de serviço (mu)\n",
    "    \n",
    "    mean_clients_values = []\n",
    "    erlang_b_values = []\n",
    "    rho_values = []\n",
    "\n",
    "    while True:\n",
    "        rho = lamb / (S * mu)  # Note que agora calculamos rho dividindo por S\n",
    "        mean_clients = mean_number_waiting_customers(lamb, mu, S)\n",
    "        erlang_b_value = erlang_b(lamb/mu, S)  # Erlang B usa lambda/mu (intensidade de tráfego)\n",
    "        \n",
    "        print(f\"rho = {rho:.4f}, mean_clients = {mean_clients:.4f}, erlang_b = {erlang_b_value:.4f}\")\n",
    "        if erlang_b_value <= 9 * 1e-3:\n",
    "            break\n",
    "        lamb -= 1\n",
    "        mu += 1\n",
    "        mean_clients_values.append(mean_clients)\n",
    "        erlang_b_values.append(erlang_b_value)\n",
    "        rho_values.append(rho)\n",
    "\n",
    "    # Ordenando os valores do maior para o menor Erlang B\n",
    "    sorted_indices = sorted(range(len(erlang_b_values)), key=lambda i: -erlang_b_values[i])\n",
    "    erlang_b_sorted = [erlang_b_values[i] for i in sorted_indices]\n",
    "    mean_clients_sorted = [mean_clients_values[i] for i in sorted_indices]\n",
    "    rho_sorted = [rho_values[i] for i in sorted_indices]\n",
    "\n",
    "    # Criando o gráfico de barras\n",
    "    plt.figure(figsize=(12, 6))\n",
    "    \n",
    "    # Índices para as barras\n",
    "    indices = range(len(erlang_b_sorted))\n",
    "    \n",
    "    # Largura das barras\n",
    "    bar_width = 0.35\n",
    "    \n",
    "    # Barras para Erlang B\n",
    "    plt.bar([i - bar_width/2 for i in indices], erlang_b_sorted, bar_width, \n",
    "            label='Erlang B', color='red', alpha=0.7)\n",
    "    \n",
    "    # Barras para número médio de clientes\n",
    "    plt.bar([i + bar_width/2 for i in indices], mean_clients_sorted, bar_width, \n",
    "            label='Mean Clients', color='blue', alpha=0.7)\n",
    "    \n",
    "    # Linha para rho\n",
    "    plt.plot(indices, rho_sorted, label=r'$\\rho$', color='green', linestyle='--', marker='o')\n",
    "    \n",
    "    plt.xlabel('Iterações (ordenadas por Erlang B decrescente)')\n",
    "    plt.ylabel('Valuees')\n",
    "    plt.title('Evolução do Número Médio de Clientes e Erlang B (Ordenado)')\n",
    "    plt.legend()\n",
    "    plt.grid(True, axis='y')\n",
    "    plt.xticks(indices, [f\"{i+1}\" for i in indices])\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Chama a função para gerar o gráfico\n",
    "plot_mean_clients_evolution()"
